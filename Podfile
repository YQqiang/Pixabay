source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '9.0'
use_frameworks!
# 忽略引入库的所有警告
inhibit_all_warnings!

target "Pixabay" do
    pod 'Kingfisher'
    pod 'Kanna'
    pod 'SnapKit'
    pod 'RxSwift'
    pod 'RxCocoa'
    pod 'Moya/RxSwift'
    pod 'ObjectMapper'
    pod 'Reveal-SDK','~>4', :configurations => ['Debug']
end
